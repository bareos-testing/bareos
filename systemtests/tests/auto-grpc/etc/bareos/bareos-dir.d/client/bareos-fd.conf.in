Client {
  Name = bareos-fd
  Description = "Client resource of the Director itself (grpc)."
  Address = @hostname@
  Password = "@fd_password@"          # password for FileDaemon
  Port = @fd_port@
}

Client {
  Name = bareos-fd2
  Description = "Client resource of the Director itself (python)."
  Address = @hostname@
  Password = "@fd_password@"          # password for FileDaemon
  Port = @fd2_port@
}
